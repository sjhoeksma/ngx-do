    "express-session": "^1.15.6",
    "express-validator": "^5.3.0",
    "gaze": "^1.1.3",
    "greenlock-express": "^2.4.6",
    "json-concat": "0.0.1",
    "json-parse-helpfulerror": "^1.0.3",
    "json-server": "^0.14.0",
    "jsonic": "^0.3.0",
    "jsonwebtoken": "^8.2.2",
    "minimist": "^1.2.0",
    "mongodb": "^3.1.8",
    "npm-watch": "^0.3.0",
    "path": "^0.12.7",
    "request": "^2.88.0",
    "rotating-file-stream": "^1.3.9",
    "server-destroy": "^1.0.1",
    "uuid": "^3.3.2",
    "tingodb": "^0.6.1"
  }
