  "license": "MIT",
  "main": "ngx-do-api-gateway.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/sjhoeksma/ngx-do.git"
  },
  "watch": {
    "concat:mocks": {
      "patterns": [
        "data"
      ],
      "extensions": "json"
    }
  },
  "scripts": {
    "start": "node index.js",
    "build": "npm run inc_patch && rm -rf ../../dist/ngx-do-api-gateway && mkdir -p ../../dist/ngx-do-api-gateway && cp ./{package.json,package-lock.json,index.js,ngx-do-api-gateway.js,ngx-do-api-gateway-routes.json,README.md} ../../dist/ngx-do-api-gateway && cp -R ./core ../../dist/ngx-do-api-gateway ",
    "inc_patch": "npm version patch -f"
  },
  "private": false,
  "dependencies": {
    "azure-ad-jwt": "^1.1.0",
    "core-js": "^2.5.7",
    "ddos": "git+https://github.com/sjhoeksma/node-ddos.git",
